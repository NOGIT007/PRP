# PRP Project Environment Variables
# Copy this file to .env and fill in your actual values

# Project Settings
PROJECT_NAME="My PRP Project"
ENVIRONMENT="development"
DEBUG=true
LOG_LEVEL="INFO"

# Security
SECRET_KEY="change-this-to-a-random-secret-key"

# Google Cloud Platform (uncomment if using GCP)
# GOOGLE_CLOUD_PROJECT="your-gcp-project-id"
# GOOGLE_APPLICATION_CREDENTIALS="path/to/service-account.json"

# Google ADK (uncomment if building agents)
# MODEL_NAME="gemini-2.5-pro"
# ADK_AGENT_NAME="your-agent-name"

# Database Connections (uncomment as needed)
# DATABASE_URL="postgresql://user:password@localhost:5432/dbname"
# NEO4J_URI="bolt://localhost:7687"
# NEO4J_USERNAME="neo4j"
# NEO4J_PASSWORD="your-neo4j-password"

# API Keys (add as needed)
# OPENAI_API_KEY="sk-your-openai-api-key"
# ANTHROPIC_API_KEY="sk-ant-your-anthropic-api-key"
# HUGGINGFACE_API_TOKEN="hf_your-huggingface-token"

# Redis (uncomment if using caching)
# REDIS_URL="redis://localhost:6379"

# Application-specific settings
# Add your custom environment variables here